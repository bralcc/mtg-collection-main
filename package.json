{
  "name": "obsidian-mtg-collection",
  "version": "0.0.1",
  "description": "Obsidian plugin for managing Magic the Gathering card collections with deck validation",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "magic-the-gathering",
    "mtg",
    "cards",
    "collection",
    "deck"
  ],
  "author": "Cedric Bral",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bralcc/mtg-collection-main.git"
  },
  "bugs": {
    "url": "https://github.com/bralcc/mtg-collection-main/issues"
  },
  "homepage": "https://github.com/bralcc/mtg-collection-main#readme",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "babel-jest": "^30.0.4",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.5",
    "jest": "^30.0.3",
    "obsidian": "latest",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
